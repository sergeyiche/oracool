{
    "name": "oracool/oracle-platform",
    "description": "Платформа для работы ИИ-агентов в мессенджерах. Агент 'Оракул' - цифровой двойник пользователя.",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-pdo_pgsql": "*",
        "doctrine/dbal": "^3.7",
        "doctrine/doctrine-bundle": "^2.11",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.16",
        "guzzlehttp/guzzle": "^7.8",
        "monolog/monolog": "^3.5",
        "openai-php/client": "^0.8",
        "predis/predis": "^2.2",
        "symfony/cache": "^6.4",
        "symfony/console": "^6.4",
        "symfony/dotenv": "^6.4",
        "symfony/flex": "^2.0",
        "symfony/framework-bundle": "^6.4",
        "symfony/http-client": "^6.4",
        "symfony/messenger": "^6.4",
        "symfony/monolog-bundle": "^3.10",
        "symfony/redis-messenger": "6.4.*",
        "symfony/routing": "^6.4",
        "symfony/runtime": "6.4.*",
        "symfony/serializer": "^6.4",
        "symfony/validator": "^6.4",
        "symfony/yaml": "^6.4"
    },
    "require-dev": {
        "symfony/debug-bundle": "^6.4",
        "symfony/stopwatch": "^6.4",
        "symfony/web-profiler-bundle": "^6.4",
        "phpunit/phpunit": "^10.5",
        "symfony/maker-bundle": "^1.52"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.4.*"
        }
    }
}
